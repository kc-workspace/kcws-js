{
  "name": "@kcws/dtcheck",
  "version": "0.24.0",
  "description": "datatype checking",
  "type": "commonjs",
  "main": "lib-commonjs/index.js",
  "module": "lib/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./lib-commonjs/index.js",
      "import": "./lib/index.js",
      "default": "./lib-umd/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "license": "SEE LICENSE IN LICENSE",
  "homepage": "https://github.com/kc-workspace/kcws-js/tree/main/packages/dtcheck",
  "repository": {
    "type": "git",
    "url": "https://github.com/kc-workspace/kcws-js.git",
    "directory": "packages/dtcheck"
  },
  "author": {
    "name": "Kamontat Chantrachirathumrong",
    "email": "kcws@kc.in.th",
    "url": "https://github.com/kamontat"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "dtcheck",
    "checker",
    "datatype"
  ],
  "files": [
    "CHANGELOG.json",
    "CHANGELOG.md",
    "dist/**/*.d.ts",
    "lib/**/*.js",
    "lib/**/*.js.map",
    "!lib/**/*.test.js",
    "!lib/**/*.test.js.map",
    "lib-commonjs/**/*.js",
    "lib-commonjs/**/*.js.map",
    "!lib-commonjs/**/*.test.js",
    "!lib-commonjs/**/*.test.js.map",
    "lib-umd/**/*.js",
    "lib-umd/**/*.js.map",
    "!lib-umd/**/*.test.js",
    "!lib-umd/**/*.test.js.map"
  ],
  "scripts": {
    "_phase:build": "heft build",
    "_phase:test": "heft test",
    "_phase:clean": "heft clean"
  },
  "devDependencies": {
    "@kcws/web-rig": "workspace:*",
    "@kcws/lintstaged-config": "workspace:*",
    "@kcws/eslint-config": "workspace:*",
    "@rushstack/heft": "0.63.4",
    "@types/generic": "workspace:*"
  },
  "engines": {
    "node": ">=12"
  }
}
