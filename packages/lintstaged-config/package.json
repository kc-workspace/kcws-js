{
  "name": "@kcws/lintstaged-config",
  "version": "0.32.0",
  "description": "a lint-staged configuration for kcws-js",
  "main": "lib/index.js",
  "module": "lib-esnext/index.js",
  "types": "dist/index.d.ts",
  "license": "SEE LICENSE IN LICENSE",
  "homepage": "https://github.com/kc-workspace/kcws-js/tree/main/packages/lintstaged-config",
  "repository": {
    "type": "git",
    "url": "https://github.com/kc-workspace/kcws-js.git",
    "directory": "packages/lintstaged-config"
  },
  "author": {
    "name": "Kamontat Chantrachirathumrong",
    "email": "kcws@kc.in.th",
    "url": "https://github.com/kamontat"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "linted-staged"
  ],
  "files": [
    "CHANGELOG.json",
    "CHANGELOG.md",
    "dist/**/*.d.ts",
    "lib/**/*.js",
    "lib/**/*.js.map",
    "!lib/**/*.test.js",
    "!lib/**/*.test.js.map",
    "lib-esnext/**/*.js",
    "lib-esnext/**/*.js.map",
    "!lib-esnext/**/*.test.js",
    "!lib-esnext/**/*.test.js.map",
    "lib-umd/**/*.js",
    "lib-umd/**/*.js.map",
    "!lib-umd/**/*.test.js",
    "!lib-umd/**/*.test.js.map"
  ],
  "scripts": {
    "build": "heft build",
    "test": "heft test",
    "clean": "heft clean",
    "_phase:build": "heft run --only build -- --clean",
    "_phase:test": "heft run --only test"
  },
  "dependencies": {
    "micromatch": "4.0.5"
  },
  "devDependencies": {
    "@kcws/node-rig": "workspace:*",
    "@kcws/eslint-config": "workspace:*",
    "@rushstack/heft": "0.64.7",
    "@types/micromatch": "4.0.6"
  },
  "engines": {
    "node": ">=12"
  }
}
