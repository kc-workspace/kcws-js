{
  "name": "@kcws/lintstaged-config",
  "version": "0.4.0",
  "description": "a lint-staged configuration for kcws-js",
  "type": "commonjs",
  "main": "lib-commonjs/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./lib-es6/index.js",
      "require": "./lib-commonjs/index.js",
      "default": "./lib-umd/index.js",
      "node": "./lib-es6/index.js"
    }
  },
  "license": "SEE LICENSE IN LICENSE",
  "homepage": "https://github.com/kc-workspace/kcws-js/tree/main/packages/lintstaged-config",
  "repository": {
    "type": "git",
    "url": "https://github.com/kc-workspace/kcws-js.git",
    "directory": "packages/lintstaged-config"
  },
  "author": {
    "name": "Kamontat Chantrachirathumrong",
    "email": "kcws@kc.in.th",
    "url": "https://github.com/kamontat"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "linted-staged"
  ],
  "files": [
    "CHANGELOG.json",
    "CHANGELOG.md",
    "dist/**/*.d.ts",
    "lib-commonjs/**/*.js",
    "lib-commonjs/**/*.js.map",
    "!lib-commonjs/**/*.test.js",
    "!lib-commonjs/**/*.test.js.map",
    "lib-es6/**/*.js",
    "lib-es6/**/*.js.map",
    "!lib-es6/**/*.test.js",
    "!lib-es6/**/*.test.js.map",
    "lib-umd/**/*.js",
    "lib-umd/**/*.js.map",
    "!lib-umd/**/*.test.js",
    "!lib-umd/**/*.test.js.map"
  ],
  "scripts": {
    "build": "heft build",
    "test": "heft test",
    "clean": "heft clean"
  },
  "dependencies": {
    "micromatch": "4.0.5"
  },
  "devDependencies": {
    "@kcws/node-rig": "workspace:*",
    "@kcws/eslint-config": "workspace:*",
    "@rushstack/heft": "0.48.9",
    "@types/heft-jest": "1.0.3",
    "@types/node": "14.14.45",
    "@types/micromatch": "4.0.2"
  }
}
