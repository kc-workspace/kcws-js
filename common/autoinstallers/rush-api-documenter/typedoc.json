{
  "$schema": "https://typedoc.org/schema.json",
  "extends": ["./typedoc.base.json"],

  // Input: https://typedoc.org/options/input/
  "name": "@kcws/js",
  "entryPointStrategy": "packages",
  "entryPoints": [
    "../../../packages/color",
    "../../../packages/dtcheck",
    "../../../packages/equals",
    "../../../packages/error",
    "../../../packages/mixin",
    "../../../packages/random",
    "../../../packages/astro-decap-cms",
    "../../../packages/github-actions",
    "../../../packages-config/eslint-config",
    "../../../packages-config/lintstaged-config",
    "../../../packages-internal/heft-esbuild-plugin",
    "../../../packages-type/*"
  ],

  // Output: https://typedoc.org/options/output/
  "out": "../../../_site",
  "emit": "docs",
  "lightHighlightTheme": "solarized-light",
  "darkHighlightTheme": "solarized-dark",
  "cname": "js.kcws.kamontat.net",
  "lang": "en",
  "githubPages": true,
  "hideParameterTypesInTitle": true,
  "hideGenerator": false,
  "searchInComments": true,
  "cleanOutputDir": true,
  "navigationLinks": {
    "Github": "https://github.com/kc-workspace/kcws-js"
  },
  "sidebarLinks": {
    "Github": "https://github.com/kc-workspace/kcws-js"
  },
  "navigation": {
    "includeCategories": true,
    "includeGroups": true,
    "includeFolders": false
  },
  "visibilityFilters": {
    "inherited": true,
    "protected": true,
    "private": false,
    "external": false,
    "@beta": true,
    "@alpha": false
  },
  "cacheBust": true

  // Other: https://typedoc.org/options/other/
  // ,"logLevel": "Verbose"
}
